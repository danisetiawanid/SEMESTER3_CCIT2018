<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dani.lakeviewcoffee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kopilogoalpikasi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kopilogoalpikasi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.dani.lakeviewcoffee.ActivityCoffeeOrderHistory"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.dani.lakeviewcoffee.ActivityCoffeeOrder"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.dani.lakeviewcoffee.ActivityCoffeeModify"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="com.dani.lakeviewcoffee.ActivityCoffeeHome"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dani.lakeviewcoffee.ActivityCoffeeProduct"
            android:label="@string/app_name"
            android:theme="@style/AppThemenobar"></activity>
    </application>

</manifest>